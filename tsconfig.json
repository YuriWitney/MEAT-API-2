{
    "compilerOptions": {
        "outDir": "dist",
        "target": "es2015",
        "module": "commonjs",
        "skipLibCheck": true, //gambiarra
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": false, //gambiarra
        "noFallthroughCasesInSwitch": true,
        "forceConsistentCasingInFileNames": true,
    },
    "exclude": [
        "**/*.test.ts"
    ]
}